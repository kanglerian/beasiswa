<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <style>
    table, tr, th, td {
      border: 1px solid black;
    }
  </style>
</head>
<body>
  <div>
    <table>
      <tr>
        <th>No</th>
        <th>Fullname</th>
        <th>School</th>
        <th>Class</th>
        <th>#</th>
      </tr>
      <% data.forEach((da,i) => { %>
      <tr>
        <td><%= i + 1 %></td>
        <td><%= da.fullName %></td>
        <td><%= da.schools %></td>
        <td><%= da.class %></td>
        <td>
          <a href="/students/<%= da.id %>"><button>detail</button></a>
          <button>edit</button>
          <button>hapus</button>
        </td>
      </tr>
      <% }); %>
    </table>
  </div>
</body>
</html>